{
  "name": "@identity.com/gateway-eth-ts",
  "version": "0.0.2",
  "description": "Adapter library for Identity.com gateway token system on Ethereum",
  "main": "dist/index.js",
  "scripts": {
    "precompile": "rm -rf dist",
    "compile": "tsc -p tsconfig.json",
    "lint": "eslint lib/ src/ test/ --ext .ts",
    "build": "npm run compile && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postbuild": "rm -f oclif.manifest.json",
    "test": "ts-mocha src/*.test.ts src/**/*.test.ts",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "version": "oclif-dev readme"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/secured-finance/gateway-eth-ts.git"
  },
  "author": "Bach Adylbekov <bach@secured-finance.com>",
  "bin": {
    "gateway-eth-ts": "./bin/run"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/secured-finance/gateway-eth-ts/issues"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/secured-finance/gateway-eth-ts#readme",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "bignumber.js": "^9.0.1",
    "ethers": "^5.4.2",
    "gas-price-oracle": "^0.3.3",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "oclif": "^1.18.1",
    "prettier": "2.3.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "oclif": {
    "commands": "./dist/commands",
    "bin": "gateway-eth-ts",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "types": "dist/index.d.ts",
  "ts-node": {
    "files": true
  }
}
