{
    "name": "@identity.com/solana-gateway-ts-v2",
    "version": "0.8.2",
    "description": "Typescript adapters for interacting with Solana gateway program",
    "main": "dist/index",
    "types": "dist/index",
    "repository": "git@github.com:identity-com/on-chain-identity-gateway.git",
    "homepage": "https://www.identity.com",
    "author": "Kevin Colgan <kevin@civic.com>",
    "license": "MIT",
    "scripts": {
      "build": "yarn clean && yarn compile && yarn doc",
      "clean": "rimraf -rf ./dist",
      "compile": "tsc -p tsconfig.build.json",
      "prepare": "yarn build",
      "test": "STAGE=${STAGE:-dev} FORCE_COLOR=1 nyc --reporter=text ts-mocha --paths 'tests/**/*.ts'",
      "lint": "eslint src/ --ext .ts",
      "lint:fix": "yarn lint --fix",
      "doc": "yarn typedoc src/index.ts --readme none",
      "create-fixtures": "anchor run createTestFixtureAccount",
      "start-validator": "solana-test-validator --bpf-program FSgDgZoNxiUarRWJYrMDWcsZycNyEXaME5i3ZXPnhrWe ./target/deploy/program_v2.so --reset"
    },
    "dependencies": {
      "@project-serum/anchor": "^0.25.0",
      "@solana/web3.js": "^1.41.3",
      "async-retry": "^1.3.3",
      "bn.js": "^5.2.0",
      "borsh": "^0.4.0",
      "chai-as-promised": "^7.1.1",
      "ramda": "^0.27.1"
    },
    "devDependencies": {
      "@types/bn.js": "^5.1.0",
      "@types/chai": "^4.2.16",
      "@types/chai-as-promised": "^7.1.3",
      "@types/chai-subset": "^1.3.3",
      "@types/expect": "^24.3.0",
      "@types/mocha": "^8.2.2",
      "@types/ramda": "^0.27.60",
      "@types/sinon": "^10.0.0",
      "@types/sinon-chai": "^3.2.5",
      "@typescript-eslint/eslint-plugin": "^4.22.0",
      "@typescript-eslint/parser": "^4.22.0",
      "chai": "^4.2.0",
      "chai-as-promised": "^7.1.1",
      "chai-subset": "^1.6.0",
      "cross-fetch": "^3.1.4",
      "eslint": "^7.25.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-config-standard": "^16.0.2",
      "eslint-plugin-import": "^2.22.1",
      "eslint-plugin-mocha": "^8.1.0",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-prettier": "^3.4.0",
      "eslint-plugin-promise": "^5.1.0",
      "eslint-plugin-react": "^7.23.2",
      "eslint-plugin-standard": "^5.0.0",
      "fetch-mock": "^9.11.0",
      "mocha": "^8.3.2",
      "mocha-circleci-reporter": "^0.0.3",
      "nyc": "^15.1.0",
      "prettier": "^2.2.1",
      "rimraf": "~3.0.2",
      "sinon": "^9.2.3",
      "sinon-chai": "^3.5.0",
      "ts-mocha": "^8.0.0",
      "ts-node": "^10.8.1",
      "typedoc": "^0.21.5",
      "typescript": "^4.2.4"
    }
  }
  