  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build": "tsc --build",
    "test": "ts-mocha --paths \"test/**/*.ts\""
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/DiosAkhilleus/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
