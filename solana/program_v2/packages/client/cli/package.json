{
  "name": "@identity.com/gateway-solana-cli",
  "version": "2.0.0-beta.13",
  "description": "Gateway V2 CLI",
  "author": "Identity.com",
  "bin": {
    "gateway": "./bin/run"
  },
  "homepage": "https://github.com/identity-com/on-chain-identity-gateway",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "identity-com/on-chain-identity-gateway",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@identity.com/gateway-solana-client": "^2.0.0-beta.13",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "@coral-xyz/anchor": "^0.27.0",
    "@solana/web3.js": "^1.50.1"
  },
  "devDependencies": {
    "oclif": "^3",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "gateway",
    "dirname": "commands",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "gateway": {
        "description": "CLI for Gateway V2"
      }
    }
  },
  "scripts": {
    "build": "tsc --build",
    "prepack": "yarn install && yarn build && oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
