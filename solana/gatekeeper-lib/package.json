{
  "name": "@identity.com/solana-gatekeeper-lib",
  "description": "Library and CLI to manage Gateway Tokens",
  "version": "1.0.16",
  "author": "dankelleher @dankelleher",
  "bin": {
    "gateway": "./bin/run"
  },
  "bugs": "https://github.com/identity-com/ociv/issues",
  "dependencies": {
    "@identity.com/solana-gateway-ts": "^0.1.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@solana/spl-token": "^0.1.4",
    "@solana/web3.js": "^1.11.0",
    "borsh": "^0.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "bs58": "^4.0.1",
    "chai": "^4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "oclif": "^1.17.0",
    "postinstall-prepare": "^2.0.0",
    "prettier": "^2.3.0",
    "ts-node": "^8",
    "typescript": "^4.2.3",
    "yargs": "^17.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/identity-com/ociv",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "gateway",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "identity-com/gatekeeper-lib",
  "scripts": {
    "build": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build",
    "start-validator": "solana-test-validator --bpf-program gatem74V238djXdzWnJf94Wo1DcnuGkfijbf3AuBhfs ../program/target/deploy/solana_gateway_program.so --reset",
    "lint": "eslint src/ --ext .ts",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
