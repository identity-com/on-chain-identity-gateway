{
  "name": "@identity.com/solana-gatekeeper-lib",
  "description": "Library to manage Gateway Tokens",
  "version": "6.1.0-alpha.2",
  "author": "dankelleher @dankelleher",
  "bugs": "https://github.com/identity-com/on-chain-identity-gateway/issues",
  "dependencies": {
    "@identity.com/solana-gateway-ts": "0.12.0-alpha.2",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/spl-token": "^0.3.5",
    "@solana/web3.js": "^1.61.0",
    "async-retry": "^1.3.3",
    "borsh": "^0.4.0",
    "ramda": "^0.28.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.3",
    "@types/bn.js": "^5.1.0",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "audit-ci": "^5.1.2",
    "bs58": "^4.0.1",
    "buffer": "^6.0.3",
    "chai": "^4",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "postinstall-prepare": "^2.0.0",
    "prettier": "^2.3.0",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "ts-mocha": "^9.0.2",
    "ts-node": "^8",
    "typedoc": "^0.21.5",
    "typescript": "^4.2.3",
    "yargs": "^17.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://www.identity.com",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "bin": "gateway",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-version",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "git@github.com:identity-com/on-chain-identity-gateway.git",
  "scripts": {
    "build": "rm -rf dist && tsc -b",
    "posttest": "yarn lint",
    "prepack": "yarn build",
    "start-validator": "solana-test-validator --bpf-program gatem74V238djXdzWnJf94Wo1DcnuGkfijbf3AuBhfs ../program/target/deploy/solana_gateway.so --reset",
    "lint": "eslint '{src,test}/**/*.ts' --max-warnings 0",
    "lint:fix": "yarn lint --fix",
    "test": "STAGE=${STAGE:-dev} FORCE_COLOR=1 nyc --extension .ts --reporter=text ts-mocha --timeout 30000 --paths 'test/**/*.ts'",
    "doc": "yarn typedoc src/index.ts --readme none --excludeNotDocumented",
    "audit-ci": "audit-ci --config audit-ci.json"
  },
  "types": "dist/index.d.ts"
}
