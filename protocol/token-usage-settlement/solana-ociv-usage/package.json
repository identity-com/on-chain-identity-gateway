{
  "name": "@identity.com/solana-ociv-usage",
  "version": "0.0.1",
  "description": "Gateway Token Usage Oracle for the Solana Gateway Program",
  "repository": "identity-com/gatekeeper-lib",
  "author": "Martin Riedel @rado0x54",
  "license": "MIT",
  "bugs": "https://github.com/identity-com/ociv/issues",
  "bin": {
    "gateway-usage": "./bin/run"
  },
  "dependencies": {
    "@identity.com/gateway-usage": "^0.0.1",
    "@oclif/core": "^1.6.3",
    "@oclif/plugin-help": "^5.1.12",
    "@solana/web3.js": "^1.50.1",
    "aws-sdk": "^2.1123.0",
    "eslint-config-oclif": "^4.0.0",
    "ramda": "^0.27.1",
    "ts-mocha": "^8.0.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.1.0",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/ramda": "^0.27.56",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4",
    "eslint": "^7.26.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^5",
    "nyc": "^14",
    "oclif": "^2.6.1",
    "prettier": "^2.3.0",
    "shx": "^0.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "dist/index.js",
  "oclif": {
    "topicSeparator": " ",
    "commands": "./dist/commands",
    "bin": "gateway-usage",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "clean": "rimraf -rf ./dist",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "dist/index.d.ts"
}
